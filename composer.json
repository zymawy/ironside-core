{
    "name": "zymawy/ironside",
    "description": "Useful classes you can use for every Laravel project",
	"keywords": [
      "laravel",
      "Ironside",
      "traits",
      "models",
      "helpers",
      "admin",
      "cms",
      "pages",
      "photos",
      "page builder",
      "controllers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hamza Zymawy",
            "email": "zymawy@gmail.com"
        }
    ],
    "require": {
      "php": "^7.1.3",
      "google/recaptcha": "^1.2",
      "guzzlehttp/guzzle": "^6.3",
      "intervention/image": "^2.4",
      "bpocallaghan/alert": "^1.1",
      "bpocallaghan/notify": "^2.1",
      "bpocallaghan/sluggable": "^1.2",
      "spatie/laravel-analytics": "^3.6",
      "bpocallaghan/impersonate": "^1.0",
      "yajra/laravel-datatables-oracle": "^8.9"
    },
  "require-dev": {
    "bpocallaghan/generators": "^5.0",
    "barryvdh/laravel-ide-helper": "^2.5"
  },
  "autoload": {
    "classmap": [
      "database/seeds"
    ],
    "psr-4": {
      "Zymawy\\Ironside\\": "app/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zymawy\\Ironside\\IronsideServiceProvider"
      ]
    }
  }
}
